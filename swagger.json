{
  "openapi": "3.0.0",
  "info": {
    "title": "Express File Store API",
    "version": "1.0.0",
    "description": "Express API"
  },
  "paths": {
    "/api/v1/blog": {
      "get": {
        "summary": "Returns All Blog",
        "responses": {
          "200": {
            "description": "List of Blogs"
          }
        }
      }
    },

    "/api/v1/admin/blog/category": {
      "post": {
        "summary": "Create a New Blog Category",
        "operationId": "createblogCategory",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "title": {
                    "type": "string",
                    "example": "Category Title"
                  }
                },
                "required": ["title"]
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Item created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "title": {
                      "type": "string",
                      "example": "Category Title"
                    }
                  },
                  "required": ["title"]
                }
              }
            }
          },
          "400": {
            "description": "Invalid input"
          }
        }
      }
    }
  }
}
